{
    "name": "zplus/theme",
    "description": "ZPlus Custom Theme for Bagisto",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zplus Team",
            "email": "info@zplus.vn"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Zplus\\Theme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zplus\\Theme\\Providers\\ThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}